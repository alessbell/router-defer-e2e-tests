{
  "name": "router-defer-e2e-tests",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@apollo/client": "3.8.3",
    "graphql": "16.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.15.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.0.1",
    "@types/node": "18.17.15",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "cypress": "12.17.4",
    "eslint-plugin-cypress": "2.14.0",
    "start-server-and-test": "2.0.0",
    "typescript": "4.9.5",
    "web-vitals": "3.4.0"
  },
  "scripts": {
    "cy:run": "cypress run",
    "cy:run-local": "cypress open",
    "test": "start-server-and-test start http://127.0.0.1:3000 cy:run",
    "test:local": "start-server-and-test start http://127.0.0.1:3000 cy:run-local",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "router:up": "cd ../scripts && make docker-build-local-router && make docker-up-local-router && cd ../CRA-demo",
    "router:down": "cd ../scripts && make docker-compose-down && cd ../CRA-demo",
    "router:restart": "npm run router:down && npm run router:up"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "cypress/no-unnecessary-waiting": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
